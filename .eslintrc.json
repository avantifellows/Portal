{
    "env": {
        "browser": true,
        "es2021": true
    },



    "plugins": [
        "vue",
        "prettier"
    ],
    "rules": {
        "vue/multi-word-component-names": ["error", {
            "ignores": ["Home", "Entry"]
          }],
          "vue/props-casing": ["camelCase"],
          "comma-dangle": "off",
        "space-before-blocks": "off"
    }
}
